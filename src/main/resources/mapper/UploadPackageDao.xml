        <result property="retain5" column="retain5" jdbcType="VARCHAR"/>
        <result property="retain6" column="retain6" jdbcType="VARCHAR"/>
    </resultMap>



    <select id="getVersion" resultMap="UploadPackageMap">
        select
               *
        from upload_package
        where status=0 or dev_id=#{devId}
        order by upload_time desc
        limit 1
    </select>

    <select id="getLockVersion" resultMap="UploadPackageMap">
        select
        *
        from upload_package
        where dev_id=#{devId} and retain1='1'
        limit 1
    </select>

    <update id="lock" >
        update upload_package set  retain1 = '1' where  id=#{id}
    </update>

    <update id="unlock" >
        update upload_package set  retain1 = '2' where  id=#{id}
    </update>

